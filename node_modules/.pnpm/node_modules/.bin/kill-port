#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/kill-port-process@3.1.0/node_modules/kill-port-process/dist/bin/node_modules:/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/kill-port-process@3.1.0/node_modules/kill-port-process/dist/node_modules:/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/kill-port-process@3.1.0/node_modules/kill-port-process/node_modules:/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/kill-port-process@3.1.0/node_modules:/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/kill-port-process@3.1.0/node_modules/kill-port-process/dist/bin/node_modules:/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/kill-port-process@3.1.0/node_modules/kill-port-process/dist/node_modules:/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/kill-port-process@3.1.0/node_modules/kill-port-process/node_modules:/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/kill-port-process@3.1.0/node_modules:/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../kill-port-process/dist/bin/kill-port-process.js" "$@"
else
  exec node  "$basedir/../kill-port-process/dist/bin/kill-port-process.js" "$@"
fi
