#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/@shopify+theme-check-docs-updater@2.9.2/node_modules/@shopify/theme-check-docs-updater/scripts/node_modules:/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/@shopify+theme-check-docs-updater@2.9.2/node_modules/@shopify/theme-check-docs-updater/node_modules:/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/@shopify+theme-check-docs-updater@2.9.2/node_modules/@shopify/node_modules:/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/@shopify+theme-check-docs-updater@2.9.2/node_modules:/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/@shopify+theme-check-docs-updater@2.9.2/node_modules/@shopify/theme-check-docs-updater/scripts/node_modules:/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/@shopify+theme-check-docs-updater@2.9.2/node_modules/@shopify/theme-check-docs-updater/node_modules:/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/@shopify+theme-check-docs-updater@2.9.2/node_modules/@shopify/node_modules:/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/@shopify+theme-check-docs-updater@2.9.2/node_modules:/Users/maxpod/Code/runpod-shopify/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@shopify+theme-check-docs-updater@2.9.2/node_modules/@shopify/theme-check-docs-updater/scripts/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../@shopify+theme-check-docs-updater@2.9.2/node_modules/@shopify/theme-check-docs-updater/scripts/cli.js" "$@"
fi
